{
  "name": "project-metrics",
  "version": "0.1.0",
  "description": "Product Metrics",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/product-metrics.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "scripts": {
    "build": "mkdir -p dist && webpack --config config/webpack-production-config.js",
    "icons:fetch": "mkdir -p dist && curl -s https://icons.learnersguild.org/tags > dist/icons-metadata.json",
    "start": "npm run icons:fetch & node server",
    "postinstall": "npm run build",
    "test": "./node_modules/.bin/xo && NODE_ENV=test ./node_modules/.bin/jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/config/jest-preprocessor.js",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "keywords": [
    "metrics",
    "swagger"
  ],
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/LearnersGuild/project-metrics/issues"
  },
  "homepage": "https://github.com/LearnersGuild/project-metrics#readme",
  "dependencies": {
    "auth0-js": "^6.7.7",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bootstrap": "twbs/bootstrap#v4.0.0-alpha.2",
    "bootstrap-loader": "^1.0.1",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^2.0.4",
    "dotenv": "^1.2.0",
    "ejs": "^2.3.4",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.3",
    "express-jwt": "^3.3.0",
    "express-sslify": "^1.0.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "json-loader": "^0.5.4",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.12",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.3",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.2",
    "sass-resources-loader": "^1.0.1",
    "serve-static": "^1.10.0",
    "style-loader": "^0.13.0",
    "swagger-tools": "0.9.*",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0",
    "yamljs": "^0.2.4"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.16.1",
    "jest-cli": "^0.8.2",
    "piping": "^0.3.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1",
    "xo": "^0.12.1"
  },
  "engines": {
    "node": "5.5.0"
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "comma-dangle": [
        0
      ],
      "no-negated-condition": [
        0
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "new-cap": [2, {"capIsNewExceptions": ["HTTPS"]}]
    },
    "esnext": true,
    "space": true,
    "semicolon": false
  }
}
