{
  "name": "los-metrics",
  "version": "0.1.0",
  "description": "LOS Metrics",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/product-metrics.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "scripts": {
    "build": "./node_modules/.bin/babel -d lib/ src/ --ignore __tests__",
    "start": "node app.js",
    "lint": "./node_modules/.bin/xo",
    "test:run": "NODE_ENV=test ./node_modules/.bin/babel-tape-runner '**/__tests__/*.test.js' | ./node_modules/.bin/faucet",
    "test": "npm run lint && npm run test:run",
    "metrics": "npm run metrics:flow && npm run metrics:quality",
    "metrics:flow": "./node_modules/.bin/babel-node ./src/metrics/flow",
    "metrics:quality": "./node_modules/.bin/babel-node ./src/metrics/quality"
  },
  "keywords": [
    "metrics"
  ],
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/LearnersGuild/los-metrics/issues"
  },
  "homepage": "https://github.com/LearnersGuild/los-metrics#readme",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "config": "^1.21.0",
    "decamelize-keys": "^1.1.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.0",
    "mkdirp": "^0.5.1",
    "parse-link-header": "^0.4.1",
    "query-string": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-runtime": "^6.11.6",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^0.2.0",
    "faucet": "0.0.1",
    "nock": "^8.0.0",
    "tape": "^4.6.0",
    "xo": "^0.16.0"
  },
  "engines": {
    "node": "5.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "xo": {
    "ignore": [
      "lib/**/*"
    ],
    "rules": {
      "comma-dangle": [
        0
      ],
      "no-negated-condition": [
        0
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "xo/filename-case": [
        0
      ]
    },
    "esnext": true,
    "space": true,
    "semicolon": false
  }
}
